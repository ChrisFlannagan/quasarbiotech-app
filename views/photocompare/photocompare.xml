<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="loaded">
    <Page.actionBar>
        <ActionBar title="Quasar Companion">
            <NavigationButton text="Go Back" tap="onNavBtnTap"/>
        </ActionBar>
    </Page.actionBar>
    <GridLayout rows="*,100">
        <AbsoluteLayout row="0" id="p1" width="100%">
            <Image id="p2" width="{{ parentWidth }}" height="{{ parentHeight }}" left="0" top="0" src="{{ comparebase }}">
            </Image>
            <Image id="p3" width="{{ parentWidth }}" height="{{ parentHeight }}" opacity="{{ a1 }}" left="0" top="0" src="{{ comparephoto }}">
            </Image>
        </AbsoluteLayout>
        <StackLayout row="1">
            <Slider id="changeSlider" value="{{ s1 }}" />
        </StackLayout>
    </GridLayout>
</Page>